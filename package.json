{
  "name": "dappql-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "docs:dev": "turbo docs:dev",
    "docs:build": "turbo docs:build",
    "lint": "turbo lint",
    "types": "turbo types",
    "test": "turbo test",
    "clean": "turbo clean && rm -rf ./node_modules && rm -rf ./.turbo",
    "ship:core": "cd packages/core && pnpm ship",
    "ship:cli": "cd packages/cli && pnpm ship"
  },
  "devDependencies": {
    "del-cli": "^6.0.0",
    "prettier": "^3.3.3",
    "turbo": "^2.3.0",
    "version-bump-prompt": "^6.1.0"
  },
  "packageManager": "pnpm@9.0.1",
  "dependencies": {
    "@tanstack/react-query": "^5.62.11",
    "viem": "^2.21.57",
    "wagmi": "^2.14.6"
  }
}